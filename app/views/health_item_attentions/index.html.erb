<p id="notice"><%= notice %></p>

<h1>Health Item Attentions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health_item_attentions.each do |health_item_attention| %>
      <tr>
        <td><%= link_to 'Show', health_item_attention %></td>
        <td><%= link_to 'Edit', edit_health_item_attention_path(health_item_attention) %></td>
        <td><%= link_to 'Destroy', health_item_attention, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health Item Attention', new_health_item_attention_path %>
