<p id="notice"><%= notice %></p>

<h1>Health Item Records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @health_item_records.each do |health_item_record| %>
      <tr>
        <td><%= link_to 'Show', health_item_record %></td>
        <td><%= link_to 'Edit', edit_health_item_record_path(health_item_record) %></td>
        <td><%= link_to 'Destroy', health_item_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Health Item Record', new_health_item_record_path %>
